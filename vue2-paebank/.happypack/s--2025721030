import _defineProperty from 'babel-runtime/helpers/defineProperty';
import * as types from './types';

var state = {
  loading: false
};

var mutations = _defineProperty({}, types.COMMON_LOADING, function (state, payload) {
  state.loading = payload;
});

var getters = {};

var actions = {};

export default {
  state: state,
  getters: getters,
  actions: actions,
  mutations: mutations
};